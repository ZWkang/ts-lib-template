  "scripts": {
    "dev": "tsup --watch",
    "lint": "eslint --cache .",
    "test": "vitest run",
    "build": "tsup",
    "start": "tsx src/index.ts",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.22.0",
    "eslint-config-alloy": "^4.6.2",
    "prettier": "^2.7.1",
    "tsup": "^6.2.2",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
